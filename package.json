{
  "name": "react-study-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "VITE_API_ENDPOINT='http://localhost:8080' vite",
    "dev:mock": "VITE_API_MOCKING=enabled VITE_API_ENDPOINT='http://localhost:8080' vite",
    "build": "tsc && vite build",
    "lint": "npx @biomejs/biome lint ./src --write",
    "format": "npx @biomejs/biome format ./src --write",
    "preview": "vite preview",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.15.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vanilla-extract/vite-plugin": "^4.0.10",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.6.0",
    "jsdom": "^24.1.0",
    "msw": "^2.3.1",
    "react-router-dom": "^6.23.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "volta": {
    "node": "20.10.0",
    "npm": "10.2.3",
    "yarn": "4.0.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
